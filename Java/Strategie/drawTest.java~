import javax.swing.*;
import java.awt.*;

public class drawTest extends JFrame
{
    public drawTest()
    {
        setSize(200,200);
        setLocation(100,100);
        setVisible(true);
    }
    
    public void paint(Graphics g)
    {
        g.setColor(new Color(100,100,100,100));
        g.fillRect(30,40, 140, 140);
        g.setColor(new Color(255,255,255,255));
        g.fillRect(40,50, 80, 80);
    }
}
